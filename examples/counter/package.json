{
  "name": "helix-basic",
  "version": "1.0.0",
  "description": "Basic example of helix",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "budo ./src/index.ts --live --pushstate --host=localhost --port=8888 --base=/ --css ./node_modules/bulma/css/bulma.css -- -t vueify",
    "dist": "npm run browserify && npm run minify",
    "browserify": "browserify ./src/index.ts -t vueify -g uglifyify -o ./dist/bundle.js",
    "minify": "uglifyjs ./dist/bundle.js --compress --mangle -o ./dist/bundle.min.js"
  },
  "author": "Joseph Luck",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^7.0.5",
    "babel-core": "^6.23.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "browserify": "^14.1.0",
    "budo": "^9.4.7",
    "tslint-config-standard": "^4.0.0",
    "uglifyify": "^3.0.4",
    "uglifyjs": "^2.4.10",
    "vueify": "^9.4.1"
  },
  "dependencies": {
    "bulma": "^0.3.2",
    "vue": "^2.2.2"
  }
}
